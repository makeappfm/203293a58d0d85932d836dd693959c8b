<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.test.test2345"
        versionCode = "10"
        version     = "1.0.0">

    <name>test2345</name>

    <description>
        This is test2345
    </description>

    <author>Tadapp</author>

    <plugin name='cordova-plugin-splashscreen' />
    <plugin name='cordova-plugin-inappbrowser' />
    <plugin name='cordova-plugin-network-information' />

    <preference name='phonegap-version' value='cli-9.0.0' />

    <gap:plugin name='onesignal-cordova-plugin' spec='2.4.5' source='npm' />

    <preference name='android-build-tool' value='gradle' />
    <preference name="android-targetSdkVersion" value="28" />
    <preference name='orientation' value='portrait' />

    <preference name="SplashMaintainAspectRatio" value="false" />
    <preference name="SplashScreenSpinnerColor" value="#ffffff" />

    <preference name="Fullscreen" value="false" />

    <platform name="android">
            
        <icon target="res/icon/android/hdpi_1616531280_bff8da66300416e846a6.png" backgroud="res/icon/android/hdpi_1616531280_bff8da66300416e846a6.png" density="hdpi" foreground="res/icon/android/hdpi_1616531280_bff8da66300416e846a6.png" />
            
        <roundIcon target="res/icon/android/hdpi_1616531280_bff8da66300416e846a6.png" backgroud="res/icon/android/hdpi_1616531280_bff8da66300416e846a6.png" density="hdpi" foreground="res/icon/android/hdpi_1616531280_bff8da66300416e846a6.png" />
            
            
        <splash src="res/screen/android/hdpi_port_1616531331_3a6c3c912459c1ae7fde.png" density="port-hdpi" />
        <splash src="res/screen/android/ldpi_port_1616531331_3a6c3c912459c1ae7fde.png" density="port-ldpi" />
        <splash src="res/screen/android/mdpi_port_1616531331_3a6c3c912459c1ae7fde.png" density="port-mdpi" />
        <splash src="res/screen/android/xhdpi_port_1616531331_3a6c3c912459c1ae7fde.png" density="port-xhdpi" />
        <splash src="res/screen/android/hdpi_land_1616531331_3a6c3c912459c1ae7fde.png" density="land-hdpi" />
        <splash src="res/screen/android/ldpi_land_1616531331_3a6c3c912459c1ae7fde.png" density="land-ldpi"/>
        <splash src="res/screen/android/mdpi_land_1616531331_3a6c3c912459c1ae7fde.png" density="land-mdpi"/>
        <splash src="res/screen/android/xhdpi_land_1616531331_3a6c3c912459c1ae7fde.png" density="land-xhdpi"/>
    </platform>

    <access origin='*'/>
    <plugin name='cordova-plugin-whitelist' />
    <allow-intent href='http://*/*'/>
    <allow-intent href='https://*/*'/>
    <allow-intent href='tel:*'/>
    <allow-intent href='sms:*'/>
    <allow-intent href='mailto:*'/>
    <allow-intent href='geo:*'/>

</widget>
